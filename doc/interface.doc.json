/userlogin 登陆 POST
{
  "username":"字符串 昵称",
  "password":"字符串 密码"
}


/addscore 提交射箭成绩
{
  "userId":"字符串 用户ID",
  "arrowRoadStandard":"箭道规格 10/18/30",
  "arrowCount":"箭组数量 ",
  "clubId":"字符串 俱乐部ID",
  "avgeragePoint":"平均环数",
  "totalPoint":"整型 总环数",
  "archeryList":"数组 每一箭的环数 [10,10,2,3,1,7,8]",
  "bullseye":"整型 10+ 的次数"
}

//范例
{
  "userId":"0001",
  "arrowRoadStandard":"18",
  "arrowCount":"12 ",
  "clubId":"1",
  "totalPoint":90,
  "archeryList":[10,10,2,3,1,7,8,6,8,9,8,7],
  "bullseye":2,
  "avgeragePoint":7.8
}


/clubbycity //根据城市ID获取俱乐部信息 POST
{
  "cityId":"Int 城市ID",
  "pagination" : {
    "pageNo": 1, // 页码，如果不传，则默认查询第一页
    "numPerPage": 1  //每页显示记录数
  }
}


/addclub // 添加俱乐部
{
"name":"String 俱乐部名称",
"address":"String 俱乐部地址",
"cperson":"String 联系人",
"cphone":"String 联系人电话",
"phone":"String 门店电话",
"province":"Int 省份",
"city":"Int 城市",
"district":"Int 区域",
"arrowRoadSize":"Int 箭道数量",
